<div class="card-container">
  <div class="card3"></div>
  <div class="card3"></div>
  <div class="card2"></div>
  <div class="card">
    <div class="card-main"><img src="{{card.img}}" class="profile-image" />
      <div class="bottom-panel">
        <div class="name-container">
          <div class="name">{{card.name}}</div>
          <div class="age">{{card.age}}</div>
        </div>
        <div class="actions-container">
            {{#each actions}}
            <button class="menu-icon">
                <img
                src="{{actionIcon}}"
                class="{{actionClass}}"
                />
            </button>
            {{/each}}
        </div>
      </div>
    </div>
  </div>
</div>